! Fonts
Xft.antialias: 1
Xft.autohint:  0
Xft.dpi:       125
Xft.hinting:   1
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault
Xft.rgba:      rgb

! Colors
! black
#define nord0 #2E3440
#define nord8 #88C0D0
! red
#define nord1 #3B4252
#define nord9 #81A1C1
! green
#define nord2 #434C5E
#define nord10 #5E81AC
! yellow
#define nord3 #4C566A
#define nord11 #BF616A
! blue
#define nord4 #D8DEE9
#define nord12 #D08770
! magenta
#define nord5 #E5E9F0
#define nord13 #EBCB8B
! cyan
#define nord6 #ECEFF4
#define nord14 #A3BE8C
!white
#define nord7 #8FBCBB
#define nord15 #B48EAD

*.color0: nord1
*.color1: nord11
*.color2: nord14
*.color3: nord13
*.color4: nord9
*.color5: nord15
*.color6: nord8
*.color7: nord5
*.color8: nord3
*.color9: nord11
*.color10: nord14
*.color11: nord13
*.color12: nord9
*.color13: nord15
*.color14: nord7
*.color15: nord6

*.foreground:   nord4
*.background:   nord0

*fading: 35
*fadeColor: nord3

*.scrollbar.background:		BG
*.scrollbar.foreground:		red !#bbb
*.scrollbar:			true
*.scrollbar.minimumThumb:	50
*.scrollbar*width:		10
*.saveLines:			20000

Xcursor.theme:			dmz
Xcursor.size:			28
*.cursorColor:  nord4

! Xterm
*.XTerm.*.faceName:		dejavu sans mono:size=10
*.XTerm.*.scaleHeight:		1.01
*.XTerm.*.internalBorder:	4

*.XTerm.*.selectToClipboard:	true
*.XTerm.*.utmpInhibit:		true
*.XTerm.*.pointerMode:		2

*.XTerm.*.eightBitInput:        false
*.XTerm.*.title:		xterm
*.XTerm.*.termName:		xterm-256color
*.XTerm.*.visualBell:		false

SshAskpass*inputTimeout:	15
