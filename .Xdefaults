# Thanks @jcs

#define BG			#e3e0d5
#define FG			#494949

Xcursor.theme:			dmz
Xcursor.size:			28

*.background:			BG
*.foreground:			FG
*.scrollbar.background:		BG
*.scrollbar.foreground:		#bbb

*.borderColor:			BG

! black
*.color0:			#393939
*.color8:			#525252

! red
*.color1:			#825b69
*.color9:			#bda0aa

! green
*.color2:			#69825b
*.color10:			#aabda0

! yellow
*.color3:			#82755b
*.color11:			#bdb3a0

! blue
*.color4:			#5b6982
*.color12:			#a0aabd

! magenta
*.color5:			#755b82
*.color13:			#b3a0bd

! cyan
*.color6:			#5b8275
*.color14:			#a0bdb3

! white
*.color7:			#cacaca
*.color15:			#ffffff

*.font:				-*-lucida-medium-r-*-*-20-*-*-*-*-*-*-*

*.scrollbar:			true
*.scrollbar.minimumThumb:	50
*.scrollbar*width:		10
*.saveLines:			20000


! xterm-specific settings

*.XTerm.*.faceName:		dejavu sans mono:size=8
*.XTerm.*.scaleHeight:		1.01
*.XTerm.*.internalBorder:	4

*.XTerm.*.selectToClipboard:	true
*.XTerm.*.utmpInhibit:		true
*.XTerm.*.pointerMode:		2

*.XTerm.*.eightBitInput:        false
*.XTerm.*.title:		xterm
*.XTerm.*.termName:		xterm-256color
*.XTerm.*.visualBell:		false

SshAskpass*inputTimeout:	15
